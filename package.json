{
  "name": "@mapapa/api-client",
  "version": "1.0.0",
  "description": "Openapi3 Model for Mapapa API",
  "main": "./build/openapi/mapapa.api.yaml",
  "scripts": {
    "build": "npm run generate-client && npm run test && tsc --pretty",
    "generate-client": "npm run build-model && source ./scripts/generate-client",
    "build-model": "source ./scripts/build-openapi-spec",
    "test": "nyc mocha"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "include": [
      "lib/**/*.js",
      "lib/**/*.ts"
    ],
    "reporter": [
      "cobertura",
      "text",
      "lcov"
    ],
    "require": [
      "ts-node/register"
    ],
    "all": true,
    "sourceMap": true,
    "produce-source-map": true
  },
  "dependencies": {
    "@apidevtools/swagger-cli": "^4.0.4",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "axios": "^1.1.3",
    "axios-retry": "^3.3.1",
    "swagger-merger": "^1.5.4"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.9",
    "chai": "^4.3.7",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
